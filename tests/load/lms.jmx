          <BeanShellPreProcessor guiclass="TestBeanGUI" testclass="BeanShellPreProcessor" testname="Pré-Processeur BeanShell" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import java.io.*;
import java.security.*;
import java.security.spec.*;
import javax.crypto.Cipher;
import org.apache.commons.codec.binary.Base64;

String PUBLIC_KEY_FILE = &quot;C:/Users/balet/Documents/Osiatis/Formations/LoadTest/tests/public.pem&quot;;

File f = new File(PUBLIC_KEY_FILE);
FileInputStream fis = new FileInputStream(f);
DataInputStream dis = new DataInputStream(fis);
byte[] keyBytes = new byte[(int)f.length()];
dis.readFully(keyBytes);
dis.close();

String publicKeyPEM = new String(keyBytes, &quot;UTF-8&quot;);
publicKeyPEM = publicKeyPEM.replaceAll(&quot;(-+BEGIN PUBLIC KEY-+\\r?\\n|-+END PUBLIC KEY-+\\r?\\n?)&quot;, &quot;&quot;);

Base64 b64 = new Base64();
byte [] decoded = b64.decode(publicKeyPEM);

X509EncodedKeySpec spec = new X509EncodedKeySpec(decoded);
KeyFactory kf = KeyFactory.getInstance(&quot;RSA&quot;);

PublicKey publicKey = kf.generatePublic(spec);

log.info(&quot;SALT=&quot; + vars.get(&quot;SALT&quot;)); 
String originalText = &quot;bbalet&quot; + vars.get(&quot;SALT&quot;);

byte[] cipherText = null;
try {
      Cipher cipher = Cipher.getInstance(&quot;RSA&quot;);
      cipher.init(Cipher.ENCRYPT_MODE, publicKey);
      cipherText = cipher.doFinal(originalText.getBytes());
} catch (Exception e) {
    	log.info(&quot;Error cipher&quot;); 
      e.printStackTrace();
}

byte[] encoded = Base64.encodeBase64(cipherText);
String encText = new String(encoded);
log.info(&quot;cipherText=&quot; + encText);

vars.put(&quot;PASSWORD&quot;, encText);
</stringProp>
